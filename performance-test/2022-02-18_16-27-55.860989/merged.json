{
 "execution": [
  {
   "scenario": "soak",
   "concurrency": 50,
   "ramp-up": "10m",
   "hold-for": "600m"
  }
 ],
 "scenarios": {
  "soak": {
   "script": "ivory.jmx",
   "variables": {
    "PAUSE": 15000,
    "scheme": "https",
    "host": "ivy-web-snd.azure.defra.cloud",
    "username": null,
    "password": null,
    "email": null,
    "use_address_lookup": false,
    "use_gov_pay": false,
    "gov_pay_url": null
   }
  }
 },
 "reporting": [
  {
   "module": "final_stats"
  },
  {
   "module": "console"
  }
 ],
 "modules": {
  "jmeter": {
   "path": "/Applications/apache-jmeter-5.4.1",
   "memory-xmx": "12G",
   "memory-xms": "4G"
  },
  "console": {
   "disable": false
  },
  "final_stats": {
   "disable": false
  }
 }
}
